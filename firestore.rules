rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId}/healthData/{recordId} {
      allow read, write: if request.auth.uid == userId;
    }
    match /users/{userId}/audio/{audioId} {
      allow read, write: if request.auth.uid == userId;
    }
    match /users/{userId} {
      allow read, write: if request.auth.uid == userId;
    }
  }
}
